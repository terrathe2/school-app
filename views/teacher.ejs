<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teacher</title>
  </head>
  <body>
    <a href="/">BACK</a>
    <table align="center" border="1">
      <thead>
        <tr>
          <td>ID</td>
          <td>First Name</td>
          <td>Last Code</td>
          <td>Email</td>
          <td>Action</td>
        </tr>
      </thead>
      <tbody>
        <% dataTeacher.forEach((row) => {%>
          <tr>
            <td><%= row.id%></td>
            <td><%= row.firstName%></td>
            <td><%= row.lastName%></td>
            <td><%= row.email%></td>
            <td><a href="/teacher/edit/<%= row.id%>">Edit</a> | <a href="/teacher/delete/<%= row.id%>">Delete</a> | <a href="#">Add Student</a></td>
          </tr>
        <%})%>
      </tbody>
    </table><hr />

    <form action="/teacher/insert" method="post">
      <label>First Name</label><br>
      <input type="text" name="firstName" required><br>
      <label>Last Name</label><br>
      <input type="text" name="lastName" required><br>
      <label>Email</label><br>
      <input type="email" name="email" required><br>
      <input type="submit" value="Submit">
    </form>
  </body>
</html>
