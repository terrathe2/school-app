<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit Student</title>
  </head>
  <body>
    <form action="/student/edit/<%= dataStudent.id%>" method="post">
      <!-- <input type="text" name="id" value="<%= dataStudent.id%>" hidden /> -->
      <label>First Name</label><br>
      <input type="text" name="firstName" value="<%= dataStudent.firstName%>" required><br>
      <label>Last Name</label><br>
      <input type="text" name="lastName" value="<%= dataStudent.lastName%>" required><br>
      <label>Email</label><br>
      <input type="text" name="email" value="<%= dataStudent.email%>" required><br>
      <input type="submit" value="Submit">
    </form>
  </body>
</html>
