<!DOCTYPE html>
<html>
  <head>
    <%- include partials/title.ejs%>
    <link rel="stylesheet" type="text/css" href="/css/form.css">
  </head>
  <body>
    <%- include partials/header.ejs%>
    <%- include partials/menu.ejs%>
    <content>
      <form id="form" action="/student/edit/<%= dataStudent.id%>" method="post">
        <!-- <input type="text" name="id" value="<%= dataStudent.id%>" hidden /> -->
        <label>First Name</label>
        <input type="text" name="firstName" value="<%= dataStudent.firstName%>" required><br>
        <label>Last Name</label>
        <input type="text" name="lastName" value="<%= dataStudent.lastName%>" required><br>
        <label>Email</label>
        <input type="email" name="email" value="<%= dataStudent.email%>" required><br>
        <input type="submit" value="Submit">
      </form>
    </content>
    <%- include partials/footer.ejs%>
  </body>
</html>
